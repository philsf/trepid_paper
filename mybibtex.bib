@Article{PCS04,
  Author         = {Paradis, E. and Claude, J. and Strimmer, K.},
  Title          = {A{PE}: {A}nalyses of {P}hylogenetics and {E}volution
                   in {R} language.},
  Journal        = {Bioinformatics},
  Volume         = {20},
  Number         = {2},
  Pages          = {289-90},
  abstract       = {Analysis of Phylogenetics and Evolution (APE) is a
                   package written in the R language for use in molecular
                   evolution and phylogenetics. APE provides both utility
