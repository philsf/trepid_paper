@Article{Cap05,
  Author         = {Capy, P.},
  Title          = {Classification and nomenclature of retrotransposable
                   elements.},
  Journal        = {Cytogenet Genome Res},
  Volume         = {110},
  Number         = {1-4},
  Pages          = {457-61},
  abstract       = {The classification and nomenclature of
                   retrotransposable elements is reviewed. A comparison is
                   made between the initial classification summarized in
                   Capy et al. (1997b), and the more recent proposal based
                   on the classification of the viruses (Hull, 2001).
