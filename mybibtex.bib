@Article{Jud94,
  Author         = {Judson, O. P.},
  Title          = {The rise of the individual-based model in ecology.},
  Journal        = {Trends Ecol Evol},
  Volume         = {9},
  Number         = {1},
  Pages          = {9-14},
  abstract       = {Recent advances of three different kinds are driving a
                   change in the way that modelling Is being done in
                   ecology. First, the theory of chaos tells us that
                   short-term predictions of nonlinear systems will be
                   difficult, and long-term predictions will be
                   impossible. The grave Implications this has for ecology
