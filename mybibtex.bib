  medline-da     = {20020307},
  medline-dcom   = {20020524},
  medline-edat   = {2002/02/28 10:00},
  medline-fau    = {Lozovsky, Elena R ; Nurminsky, Dmitry ; Wimmer, Ernst
                   A ; Hartl, Daniel L},
  medline-gr     = {GM33741/GM/NIGMS NIH HHS/United States ;
                   GM58423/GM/NIGMS NIH HHS/United States ;
