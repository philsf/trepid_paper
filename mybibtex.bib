@Article{MCH+74,
  Author         = {May, R. M. and Conway, G. R. and Hassell, M. P. and
                   Southwood, T. R. E.},
  Title          = {Time delays, density-dependence and single-species
                   oscillations},
  Journal        = {Journal of Animal Ecology},
  Volume         = {43},
  Number         = {3},
  Pages          = {pp. 747-770},
  abstract       = {(1) The stability criteria of a single-species
                   population with a maintained food supply depend on the
                   relationship between the characteristic return time
                   (TR) and the time delays of the system. TR expresses
                   the rate at which the population approaches the
                   equilibrium and the time delays may refer either to
                   generation time or extrinsic delays as may be
                   associated with a resource recovery time. (2) For a
                   single age-class population with discrete generations
                   where b is the slope of the density-dependent
                   relationship between mortality or survival (as a
                   k-value) and log population density, the characteristic
                   return time is 1/b. The time delay in this case is the
                   generation time (tau = 1). The population damps
                   exponentially (undercompensates) if TR is greater than
                   tau but becomes increasingly unstable (overcompensates)
                   as TR falls below tau. (3) The continuous
                   (differential) analogue of this discrete single age
                   class example is the Gompertz equation. By including in
                   this an explicit time delay (T), the stability
                   properties become very similar to the discrete case.
                   (4) General models for the discrete and continuous
                   cases are discussed. They further emphasize that a
                   general requirement for stability is that the
                   characteristic return time TR be larger than the time
                   delays (tau or T). (5) Different models for density
                   dependence are reviewed with special emphasis on
                   distinguishing the linear from non-linear models in log
                   population units. The linear models, which are adequate
                   in the neighbourhood of equilibrium, give a misleading
                   impression of the global stability properties. They
                   predict ever expanding oscillations when there is very
                   strong density-dependence (b>2). Only the non-linear
                   models permit stable limit cycles under these
                   conditions. (6) The stability properties of populations
                   when more than one age class is present at one time are
                   more complicated and in many ways are similar to those
                   of multi-species systems. The single parameter b is
                   replaced by a matrix of parameters characterizing the
                   slopes of the density dependent relationships between
                   age classes. The stability of the population in the
                   neighbourhood of the equilibrium is then set by the
                   dominant eigenvalue of this matrix.},
  copyright      = {Copyright \^A\copyright{} 1974 British Ecological
                   Society},
  issn           = {00218790},
  jstor_articletype = {research-article},
  jstor_formatteddate = {Oct., 1974},
  jstor_issuetitle = {},
  language       = {English},
  publisher      = {British Ecological Society},
  url            = {http://www.jstor.org/stable/3535},
  year           = 1974
}

@Article{Has75,
  Author         = {Hassell, M. P.},
  Title          = {Density-dependence in single-species populations},
  Journal        = {Journal of Animal Ecology},
  Volume         = {44},
  Number         = {1},
  Pages          = {pp. 283-295},
  abstract       = {(1) The general form of density-dependent
                   relationships is discussed and illustrated with
                   examples. These are compared with the relationships
                   predicted from two other models for density-dependence.
                   (2) A new model is proposed which has the properties
                   that (1) at high population densities, the relationship
                   between mortality, or natality (expressed as k-values),
                   and the log density is characterized by a constant
                   slope (b) and (2) the degree of density-dependence
                   decreases as population density falls. (3) The
                   stability properties of this model are discussed. A
                   population may exhibit exponential damping, oscillatory
                   damping, or stable limit cycle behaviour, depending
                   upon the parameter (b) and the effective rate of
                   increase of the population (lambda).},
