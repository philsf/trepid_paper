@TechReport{Koe11,
  Author         = {{Katia Koelle}},
  Title          = {Meeting {R}eport from the {RAPIDD} {W}orkshop on
                   {P}hylodynamics},
  Institution    = {NESCent},
  Address        = {Durham, NC},
  abstract       = {The term 'phylodynamics' was coined in a 2004 Science
                   review authored by Grenfell and colleagues. In this
                   review, the aim of this field's research was clearly
                   articulated: to better understand how ecological,
                   evolutionary, and immunological processes shape viral
                   phylogenies. Since 2004, this field has grown
                   exponentially, with over 450 publications to date
                   contributing to its goals. A cursory overview of these
                   publications indicates that the field has grown in two
                   distinct directions, with little cross-talk between
                   them. The first direction has continued to focus on the
                   processes shaping viral phylogenies by implementing
                   mechanistic disease models and comparing their
                   simulated sequences or infection trees against
                   phylogenies reconstructed from empirical sequences. The
                   second direction, becoming ever more prominent, instead
                   starts with viral sequences and uses predominantly
                   coalescent approaches to reconstruct time-varying
                   infection dynamics. These reconstructed infection
                   dynamics can be compared against time series data, when
                   available, with the possibility of leading to
                   qualitative conclusions about when selective processes
                   might be at play. In May 2011, the RAPIDD program
                   funded a 5-day workshop focused on furthering the field
                   of phylodynamics. The goal of this workshop was to
                   bring together leading researchers using one or both of
                   the approaches outlined above in an attempt to more
                   effectively unify these directions. The focus was true
                   to the original aim of 'phylodynamics', and discussions
                   were therefore focused on whether processes could be
                   identified from the shapes of viral phylogenies and
                   novel statistical methods that would facilitate this
                   identification. This phylodynamic synthesis is a key
                   area in the Metadynamics Working Group. The workshop
                   was centered on participant talks, with daily
                   discussions that aimed to integrate across these talks
                   to provide a constructive way forward for the field.
                   The workshop was highly productive: from a combination
                   of the talks and discussions that the workshop
                   facilitated, several collaborative interactions emerged
                   and a synthesis paper was outlined to revisit the
                   maturation of the field of phylodynamics. A subset of
                   these collaborative interactions would greatly benefit
                   from one or two additional meetings. These would be
                   small working meetings instead of workshops and consist
                   of 3-6 individuals. },
  month          = may,
  year           = 2011
}

@Article{OKA+12,
  Author         = {Orem, J. N. and Kirigia, J. M. and Azairwe, R. and
                   Kasirye, I. and Walker, O.},
  Title          = {Impact of malaria morbidity on gross domestic product
                   in {U}ganda.},
  Journal        = {Int Arch Med},
  Volume         = {5},
  Number         = {1},
  Pages          = {12},
  abstract       = {ABSTRACT: BACKGROUND: The burden of malaria is a key
                   challenge to both human and economic development in
                   malaria endemic countries. The impact of malaria can be
                   categorized from three dimensions, namely: health,
                   social and economic. The objective of this study was to
                   estimate the impact of malaria morbidity on gross
                   domestic product (GDP) of Uganda. METHODS: The impact
                   of malaria morbidity on GDP of Uganda was estimated
                   using double-log econometric model. The 1997-2003 time
                   series macro-data used in the analysis were for 28
                   quarters, i.e. 7 years times 4 quarters per year. It
                   was obtained from national and international secondary
                   sources. RESULTS: The slope coefficient for Malaria
                   Index (M) was 0.00767; which indicates that when
                   malaria morbidity increases by one unit, while holding
                   all other explanatory variables constant, per capita
                   GDP decreases by US\$0.00767 per year. In 2003 Uganda
                   lost US\$ 49,825,003 of GDP due to malaria morbidity.
                   Dividing the total loss of US\$49.8 million by a
                   population of 25,827,000 yields a loss in GDP of
                   US\$1.93 per person in Uganda in 2003. CONCLUSION:
                   Malaria morbidity results in a substantive loss in GDP
                   of Uganda. The high burden of malaria leads to
                   decreased long-term economic growth, and works against
                   poverty eradication efforts and socioeconomic
                   development of the country.},
  language       = {ENG},
  medline-aid    = {1755-7682-5-12 [pii] ; 10.1186/1755-7682-5-12 [doi]},
