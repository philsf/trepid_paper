@Article{SG06,
  Author         = {Sinkins, S. P. and Gould, F.},
  Title          = {Gene drive systems for insect disease vectors.},
  Journal        = {Nat Rev Genet},
  Volume         = {7},
  Number         = {6},
  Pages          = {427-35},
  abstract       = {The elegant mechanisms by which naturally occurring
                   selfish genetic elements, such as transposable
                   elements, meiotic drive genes, homing endonuclease
                   genes and Wolbachia, spread at the expense of their
                   hosts provide some of the most fascinating and
                   remarkable subjects in evolutionary genetics. These
                   elements also have enormous untapped potential to be
                   used in the control of some of the world's most
                   devastating diseases. Effective gene drive systems for
                   spreading genes that can block the transmission of
                   insect-borne pathogens are much needed. Here we explore
                   the potential of natural gene drive systems and discuss
