@Article{Bro05b,
  Author         = {Brookfield, J. F.},
  Title          = {The ecology of the genome - mobile {DNA} elements and
                   their hosts.},
  Journal        = {Nat Rev Genet},
  Volume         = {6},
  Number         = {2},
  Pages          = {128-36},
  abstract       = {The genomes of multicellular eukaryotes provide
                   information that determines the phenotype. However, not
                   all sequences in the genome are required for this
                   purpose. Other sequences are often selfish in their
                   actions and interact in complex ways. Here, an analogy
                   is developed between the components of the genome,
                   including mobile DNA elements, and an ecological
                   community. Unlike ecological communities, however, the
                   slow rates at which genomes change allow us to
                   reconstruct patterns of interaction that stretch back
                   tens or hundreds of millions of years.},
